<Bindings>
    <Layer name="SI_KEYBINDINGS_LAYER_USER_INTERFACE_SHORTCUTS">
        <Category name="GuildBankStuffer">
            <Action name="GUILDBANKSTUFFER_MARK_DEPOSIT">
                <Down>GuildBankStuffer:MarkDeposit()</Down>
            </Action>
            <Action name="GUILDBANKSTUFFER_UNMARK_DEPOSIT">
                <Down>GuildBankStuffer:UnmarkDeposit()</Down>
            </Action>
            <Action name="GUILDBANKSTUFFER_START_WITHDRAW">
                <Down>GuildBankStuffer:StartTransferFromGuildBank()</Down>
            </Action>
            <Action name="GUILDBANKSTUFFER_START_DEPOSIT">
                <Down>GuildBankStuffer:StartTransferToGuildBank()</Down>
            </Action>
        </Category>
    </Layer>
</Bindings>
